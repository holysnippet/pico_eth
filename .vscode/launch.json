    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/pico_eth.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "Pico2040",
            //"runToMain": true,
            "configFiles": ["interface/picoprobe.cfg", "target/rp2040.cfg"],
            "searchDir": ["C:\\VSARM\\openocd\\tcl"],
            "svdFile": "C:\\VSARM\\sdk\\pico\\pico-sdk\\src\\rp2040\\hardware_regs\\rp2040.svd",
        },
        {
            "name": "Cortex Run",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/pico_eth.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "Pico2040",
            //"runToMain": true,
            "stopOnEntry": false,
            "stopAtEntry": false,
            "configFiles": ["interface/picoprobe.cfg", "target/rp2040.cfg"],
            "searchDir": ["C:\\VSARM\\openocd\\tcl"],
            "svdFile": "C:\\VSARM\\sdk\\pico\\pico-sdk\\src\\rp2040\\hardware_regs\\rp2040.svd",
        }
    ]
